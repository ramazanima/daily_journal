<%- include("partials/header"); %>

<h1>Compose</h1>
<form class="" action="compose", method="post">
    <div class="form-group">
    <label for="postTitle"><b>Title</b></label>
    <input class="form-control" type="text" name="postTitle">
    <label for="postBody"><b>Post</b></label>
    <textarea class="form-control" name="postBody" rows="4" cols="50">  
    </textarea>
    </div>
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>


